- provide(:title, "Editing Company") 
%h1 New Company

.row
  .span6.offset3
    = form_for @company do |f| 
      = render 'shared/error_messages', object: f.object
      = f.label :name 
      = f.text_field :name 
      = f.label :number 
      = f.text_field :number  

      = f.submit "Update company!", class: "btn btn-large btn-primary"
      = link_to "Back", :back, class: "btn btn-large pull-right"