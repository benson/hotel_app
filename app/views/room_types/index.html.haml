- provide(:title, 'All Room Types') 
%h1 All Room Types

/ = will_paginate 
= link_to "New Room Type", new_room_type_path
%ul.room_types
  - @room_types.each do |room_type| 
    %li
      = link_to "#{room_type.name}", room_type 
      = link_to "Delete", room_type, method: :delete, data: { confirm: "You sure?" }, class: "btn btn-small"
      = link_to "Edit", edit_room_type_path(room_type), class: "btn btn-small"
/ = will_paginate 